{
  "parser": "babel-eslint",
  "rules": {
    "semi": [
      "error",
      "never"
    ],
    "max-len": ["error", { "code": 120 }],
    "comma-dangle": [
      "error",
      "never"
    ],
    "no-underscore-dangle": 0,
    "prefer-const": 0,
    "no-empty": ["error", { "allowEmptyCatch": true }],
    "eol-last": 0,
    "class-methods-use-this": 0,
    "no-trailing-spaces": 0,
    "arrow-body-style": 0,
    "no-param-reassign": 0
  },
  "env": {
    "jest": true
  },
  "extends": [
    "airbnb-base"
  ]
}
